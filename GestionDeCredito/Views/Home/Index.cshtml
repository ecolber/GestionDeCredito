@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Bienvenidos</h1>
    

    <div id="ImagenesInicio" class="carousel slide" data-bs-ride="carousel">
        <div class="carousel-inner">
            <div class="carousel-item active">
                <img src="~/imgs/GeCred3.jpg" />
            </div>
            <div class="carousel-item">
                <img src="~/imgs/GeCred2.jpg" />
               
            </div>
            <div class="carousel-item">

                <img src="~/imgs/GeCred1.jpg" />
            </div>
        </div>
    </div>
</div>

@section Scripts{ 
    <script>
        var myCarousel = document.querySelector('#ImagenesInicio')
        var carousel = new bootstrap.Carousel(myCarousel, {
            interval: 2000,
            wrap: false
        })
    </script>
}